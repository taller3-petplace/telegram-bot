{
  "pets_service":
  {
    "base": "http://localhost:8712/pets",
    "endpoints":
    {
      "register_pet":
      {
        "path": "/pet",
        "method": "POST"
      },
      "get_pets":
      {
        "path": "/owner/{ownerID}",
        "method": "GET",
        "query_params":
        {
          "offset": 0,
          "limit": 100
        }
      },
      "get_pet_by_id":
      {
        "path": "/pet/{petID}",
        "method": "GET"
      }
    }
  },
  "treatments_service":
  {
    "base": "http://localhost:8712/treatments",
    "endpoints": {
      "get_pet_treatments": {
        "path": "/treatment/pet/{petID}",
        "method": "GET",
        "query_params": {
          "offset": 0,
          "limit": 5
        }
      },
      "get_treatment": {
        "path": "/treatment/{treatmentID}",
        "method": "GET"
      }
    }
  },
  "users_service":
  {
    "base": "http://localhost:8712/users",
    "endpoints": {
      "user_fetcher":
      {
        "path": "/telegram_id/{telegramID}",
        "method": "GET"
      }
    }
  }
}